<div class="slide" 
    <% if(image == 'without-image.png' || image == 'default-avatar.png' || image == 'default-gallery.jpg' ){ %>
       data-thumb="/images/products/<%= image %>"
    <% } else { %>
        data-thumb="<%= s3Url + "/" + image %>"
    <% } %>
    >
    
    <% if(image == 'without-image.png' || image == 'default-avatar.png' || image == 'default-gallery.jpg' ){ %>
       <img src="/images/products/<%= image %>" alt="Image"/>
    <% } else { %>
        <img src="<%= s3Url + "/" + image %>" alt="Image"/>
    <% } %>
    <div class="bg-overlay">
        <div class="bg-overlay-content justify-content-start align-items-end">
            <div class="h4 font-weight-light bg-dark br-5 color text-uppercase px-3 py-2"><%= category %></div>
        </div>
    </div>
</div>